# ============================================================
# LOCAL PROPERTIES TEMPLATE
# ============================================================
# Copy this file to 'local.properties' and fill in your values.
# Do NOT commit local.properties to version control!
# ============================================================

# Android SDK location (auto-generated by Android Studio)
sdk.dir=/path/to/android/sdk

# ============================================================
# PRIVY CONFIGURATION (Authentication)
# ============================================================
# Get these from: https://console.privy.io/
PRIVY_APP_ID=your_privy_app_id_here
PRIVY_APP_CLIENT_ID=your_privy_app_client_id_here

# ============================================================
# API KEYS
# ============================================================

# Etherscan API Key (for transaction history)
# Get from: https://etherscan.io/apis
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Alchemy API Key (for token prices)
# Get from: https://www.alchemy.com/
ALCHEMY_API_KEY=your_alchemy_api_key_here

# KyberSwap Client ID (for swap rate limit elevation)
# This is your app identifier for KyberSwap Aggregator API
KYBER_CLIENT_ID=your_kyber_client_id_here
